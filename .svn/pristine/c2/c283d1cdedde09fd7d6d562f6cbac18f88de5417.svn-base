<div ng-controller="cformShowCtrl" ng-init="init()">
    <!-- BEGIN PAGE HEADER-->
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <a href="/">Home</a>
                <i class="fa fa-circle"></i>
            </li>
            <li>
                <span>{{cform.Title}}</span>
            </li>
        </ul>

    </div>
    <!-- BEGIN PAGE TITLE-->
