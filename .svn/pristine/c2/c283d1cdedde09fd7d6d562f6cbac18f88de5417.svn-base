        {{cform.Title}}
        <small>{{cform.Description}}</small>
    </h1>
    <!-- END PAGE TITLE-->
    <!-- END PAGE HEADER-->
    <ng-include src="templateSrc"></ng-include>
    
</div>

@section js {

    <script src="~/app/components/custom-form/custom-form.show.js"></script>
}